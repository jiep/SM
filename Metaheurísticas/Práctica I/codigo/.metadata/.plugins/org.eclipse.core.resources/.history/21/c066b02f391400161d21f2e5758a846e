package es.urjc.phub;

import java.io.BufferedReader;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.util.logging.Level;
import java.util.logging.Logger;

public class PHub {
	
	InstanciaPHub instanciaPHub = null;
	
	static void leerInstancia(String filepath){
		try {
            BufferedReader br = new BufferedReader(new FileReader("inputfile.txt"));
            String s=null;

            while((s=br.readLine())!=null && (s = s.trim()).length() > 0){
                 String f[] = s.split("\t");

               System.out.println(s);

            }  br.close();
        } catch (FileNotFoundException ex) {
            System.out.println("No se pudo encontrar el fichero " + filepath);
        }

    }
	}

	public static void main(String[] args) {

	}

}
